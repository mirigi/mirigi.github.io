<?php

if ( empty($_GET['make']) ) {
  echo "error \n";
  echo "make parameter missing";
  return; 
 }

$make = $_GET['make'];
$emblemfile = getcwd().'/'.$make.'.gif';
$filetosend = getcwd().'/missing.gif';

if (file_exists($emblemfile)) {
  $filetosend = $emblemfile;
 }

$fn = fopen($filetosend, 'r');
header ("Content-type: image/gif"); 
fpassthru($fn);
return;

